{
  "name": "@kiqr/node-workspace",
  "version": "0.1.0",
  "description": "Monorepo for KIQR.CLOUD, CLI, React and Node SDKs.",
  "main": "index.js",
  "repository": "git@github.com:kiqr/node-workspace.git",
  "author": "KIQR",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rush build",
    "build:cli": "rush build --to tag:cli",
    "build:cloud": "rush build --to tag:cloud",
    "kiqr-dev": "rush build --to tag:cli && kiqr $1",
    "generate:sdks": "pnpm generate:sdks:management-api-typescript",
    "generate:sdks:management-api-typescript": "openapi-generator generate -g typescript-axios -o sdks/management-api-sdk -c ./common/config/generators/management-api-typescript.yaml"
  },
  "devDependencies": {
    "@jeliasson/husky-hooks": "^0.3.1",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "husky": "^8.0.1"
  }
}
